<transformalize>
  <processes>
    <add name="RecipeSteps">
      <connections>
        <add name="input" provider="postgresql" port="5432" database="MyDD" user="mydd" password="password" />
        <add name="output" database="TflMyDD" />
      </connections>
      <entities>
        <add name="app_step" alias="Step" prefix="step_" version="modified">
          <fields>
            <add name="id" type="int32" ></add>
            <add name="recipe_id" type="int32" label="recipe id" ></add>
            <add name="sequence" type="int16" sort="asc"></add>
            <add name="description" ></add>
            <add name="created" type="datetime" ></add>
            <add name="modified" type="datetime" ></add>
          </fields>
        </add>
      </entities>
      <relationships>
        <!--<add left-entity="RecipeSteps" left-field="tag_id" right-entity="Tag" right-field="tag_id" />-->
      </relationships>
    </add>
  </processes>
</transformalize>